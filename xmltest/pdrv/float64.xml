<?xml version="1.0" ?>
<database>

<record recordName = "syncFloat64Port" extends = "portCreate">
    <scalar name = "factoryName">org.epics.ioc.pdrv.testDriver.Float64DriverFactory</scalar>
    <scalar name = "portName">syncFloat64Port</scalar>
    <structure name = "driverParameters" extends = "float64Driver" >
        <scalar name = "delay">0.0</scalar>
        <scalar name = "numberRegisters">2</scalar>
    </structure>
</record>

