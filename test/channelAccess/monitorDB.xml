<?xml version="1.0" ?>
<IOCDatabase>

<record name = "monitorCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>20.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1.0</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <valueAlarm>
        <configure structureName = "doubleAlarm">
          <active>true</active>
          <interval>
              <value structureName = "doubleAlarmInterval">
                  <value>2.0</value>
                  <severity>major</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>5.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>14.0</value>
                  <severity>none</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>17.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>20.0</value>
                  <severity>major</severity>
              </value>
          </interval>
          <outOfRange>invalid</outOfRange>
          <hystersis>0.1</hystersis>
        </configure>
    </valueAlarm>
    <displayLimit>
        <low>0.0</low>
        <high>20.0</high>
    </displayLimit>
    <scan>
        <scan>periodic</scan>
        <rate>2.0</rate>
    </scan>
</record>

<record name = "monitorPut" type = "double">
    <input supportName = "monitorLink" >
        <configure structureName = "monitorLink">
            <pvname>monitorCounter</pvname>
            <type>put</type>
            <process>true</process>
            <inheritSeverity>true</inheritSeverity>
        </configure>
    </input>
    <displayLimit>
        <low>0.0</low>
        <high>20.0</high>
    </displayLimit>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>true</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>cachePut</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
        <!-- do it twice -->
        <value structureName = "linkArrayElement">
            <wait>true</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>cachePut</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
        <value structureName = "linkArrayElement">
            <link  supportName = "delayLink" >
                <configure structureName = "delayLink">
                    <min>100</min>
                    <max>1000</max>
                    <inc>100</inc>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "cachePut" type = "double">
    <displayLimit>
        <low>0.0</low>
        <high>20.0</high>
    </displayLimit>
    <scan>
        <processSelf>true</processSelf> 
    </scan>
</record>


<record name = "monitorChange" type = "double">
    <input supportName = "monitorLink" >
        <configure structureName = "monitorLink">
            <pvname>cachePut</pvname>
            <type>change</type>
            <process>true</process>
        </configure>
    </input>
    <displayLimit>
        <low>0.0</low>
