<?xml version="1.0" ?>
<IOCDatabase>
<record name = "getPutCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>10.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1.0</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>getFromCounter</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>putFromCounter</pvname>
                <process>true</process>
            </link>
        </value>
    </supportArray>
    <valueAlarm  structureName = "doubleAlarm">
      <active>true</active>
      <interval>
          <value structureName = "doubleAlarmInterval">
              <value>2.0</value>
              <severity><choice>major</choice></severity>
          </value>
          <value structureName = "doubleAlarmInterval">
              <value>4.0</value>
              <severity><choice>minor</choice></severity>
          </value>
          <value structureName = "doubleAlarmInterval">
              <value>6.0</value>
              <severity><choice>none</choice></severity>
          </value>
          <value structureName = "doubleAlarmInterval">
              <value>8.0</value>
              <severity><choice>minor</choice></severity>
          </value>
          <value structureName = "doubleAlarmInterval">
              <value>10.0</value>
              <severity><choice>major</choice></severity>
          </value>
      </interval>
      <outOfRange><choice>invalid</choice></outOfRange>
      <hystersis>0.3</hystersis>
    </valueAlarm>
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "getFromCounter" type = "double">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "inputSupport" structureName = "inputSupport">
        <pvname>getPutCounter</pvname>
        <process>false</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>
<record name = "putFromCounter" type = "double">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
</record>


</IOCDatabase>
