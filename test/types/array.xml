            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>longArray1FromCounter</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>longArray2FromCounter</pvname>
                    <process>true</process>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>floatArray1FromCounter</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>floatArray2FromCounter</pvname>
                    <process>true</process>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>doubleArray1FromCounter</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>doubleArray2FromCounter</pvname>
                    <process>true</process>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>stringArray1FromCounter</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>stringArray2FromCounter</pvname>
                    <process>true</process>
            </link>
        </element>
    </supportArray>
    <scan structureName = "scan">
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <display structureName = "display" >
        <description>Sample Description</description>
        <format>%f</format>
        <resolution>1</resolution>
        <units>volts</units>
    </display>
</record>
<record name = "arrayAdd">
    <input structureName = "calculation" >
      <calcArgArray>
        <element structureName = "calcArg">
            <value elementType = "double"/>
            <name>a</name>
            <input supportName = "inputSupport" structureName = "inputSupport">
                <pvname>arrayIncrementer</pvname>
                <process>false</process>
                <propertyNames>alarm</propertyNames>
            </input>
            <alarm structureName = "alarm" />
        </element>
        <element structureName = "calcArg">
            <value elementType = "double" />
            <name>b</name>
            <input supportName = "inputSupport" structureName = "inputSupport">
                <pvname>arrayIncrementer</pvname>
                <process>false</process>
                <propertyNames>alarm</propertyNames>
            </input>
            <alarm structureName = "alarm" />
        </element>
      </calcArgArray>
      <calculator supportName = "arrayAddCalculator" />
    </input>
    <value elementType = "double"/>
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
</record>

