<?xml version="1.0" ?>
<IOCDatabase>

<record name = "arrayIncrementer">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <value elementType = "double" capacity = "3">
        <element>0.0 1.0 2.0</element>
    </value>
    <input structureName = "calculation" >
      <calculator supportName = "arrayIncrementCalculator" />
    </input>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>arrayAdd</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>byteArray1FromCounter</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>byteArray2FromCounter</pvname>
                    <process>true</process>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>shortArray1FromCounter</pvname>
