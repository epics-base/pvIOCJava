<?xml version="1.0" ?>
<IOCDatabase>
<record name = "booleanArrayToggle">
    <value elementType = "boolean"/>
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <value capacity = "3">
        <element>false true false</element>
    </value>
    <input structureName = "calculation" >
       <calculator supportName = "booleanArrayToggleCalculator" />
    </input>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>booleanArray1FromToggle</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>booleanArray2FromToggle</pvname>
                    <process>true</process>
            </link>
        </element>
    </supportArray>
    <scan structureName = "scan">
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "booleanArray1FromToggle">
    <value elementType = "boolean" />
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <input supportName = "inputSupport" structureName = "inputSupport">
            <pvname>booleanArrayToggle</pvname>
            <process>false</process>
    </input>
</record>
<record name = "booleanArray2FromToggle">
    <value elementType = "boolean" />
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
</record>
<record name = "booleanArray3FromToggle">
    <value elementType = "boolean" />
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
            <pvname>booleanArrayToggle</pvname>
            <process>true</process>
    </input>
</record>

</IOCDatabase>
