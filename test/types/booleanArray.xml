<?xml version="1.0" ?>
<database>
<record name = "booleanArrayToggle">
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "alarm" type = "alarm" />
    <array name = "value" type = "boolean">
        false true false
    </array>
    <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">periodic</scalar></structure>
        <scalar name = "rate">1.0</scalar>
    </structure>
    <structure name = "input" type = "calculation" >
      <structure name = "calculator" type = "generic">
          <auxInfo name = "supportFactory" type = "string">booleanArrayToggleCalculatorFactory</auxInfo>
      </structure>
    </structure>
    <structure name = "link" type = "generic" >
        <structure name = "1" type = "caProcessLink">
            <scalar name = "wait" type = "boolean" >false</scalar>
            <scalar name = "pvname">booleanArray1FromToggle</scalar>
        </structure>
        <structure name = "2" type = "caOutputLink">
            <scalar name = "wait" type = "boolean" >false</scalar>
            <scalar name = "pvname">booleanArray2FromToggle</scalar>
            <scalar name = "process">true</scalar>
        </structure>
    </structure>
</record>

<record name = "booleanArray1FromToggle">
    <array name = "value" type = "boolean" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "input" type = "caInputLink">
        <scalar name = "pvname">booleanArrayToggle</scalar>
    </structure>
</record>

<record name = "booleanArray2FromToggle" >
    <array name = "value" type = "boolean" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "alarm" type = "alarm" />
</record>
<record name = "booleanArray3FromToggle">
    <array name = "value" type = "boolean" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "input" type = "caMonitorLink">
        <scalar name = "pvname">booleanArrayToggle</scalar>
        <scalar name = "process">true</scalar>
    </structure>
</record>

</database>
