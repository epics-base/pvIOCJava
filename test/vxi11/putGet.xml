<?xml version="1.0" ?>
<database>

<record name = "putGetPort" type = "portCreate">
<scalar name = "factoryName">org.epics.ioc.pdrv.vxi11.DriverFactory</scalar>
    <scalar name = "portName">putGetPort</scalar>
    <structure name = "driverParameters" type = "vxi11Driver" >
        <scalar name = "hostName">192.168.1.10</scalar>
        <scalar name = "vxiName">hpib</scalar>
        <scalar name = "lockTimeout">1000</scalar>
    </structure>
</record>

<record name = "putGetControl" type = "portDeviceControl" >
    <scalar name = "portName">putGetPort</scalar>
    <scalar name = "deviceName">3</scalar>
<!--
    <scalar name = "traceMask">0xfff</scalar>
    <scalar name = "traceIOMask">0xfff</scalar>
-->
</record>

<record name = "putGet">
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "put" type = "generic" >
      <scalar name = "value" type = "string">*IDN?</scalar>
      <structure name = "alarm" type = "alarm" />
      <structure name = "output" type = "pdrvSupport">
        <auxInfo name = "supportFactory" type = "string">pdrvOctetOutputFactory</auxInfo>
        <scalar name = "portName">putGetPort</scalar>
        <scalar name = "deviceName">3</scalar>
        <scalar name = "timeout">.2</scalar>
      </structure>
    </structure>
    <structure name = "get" type = "generic" >
      <scalar name = "value" type = "string" />
      <structure name = "alarm" type = "alarm" />
      <structure name = "input" type = "pdrvSupport">
        <auxInfo name = "supportFactory" type = "string">pdrvOctetInputFactory</auxInfo>
        <scalar name = "portName">putGetPort</scalar>
        <scalar name = "deviceName">3</scalar>
        <scalar name = "timeout">.2</scalar>
        <scalar name = "size">80</scalar>
      </structure>
    </structure>
</record>

<record name = "putGetInInterrupt">
    <scalar name = "value" type = "string"/>
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "pdrvSupport">
        <auxInfo name = "supportFactory" type = "string">pdrvOctetInterruptInputFactory</auxInfo>
        <scalar name = "portName">putGetPort</scalar>
        <scalar name = "deviceName">3</scalar>
        <scalar name = "size">80</scalar>
        <scalar name = "process">true</scalar>
      </structure>
</record>
