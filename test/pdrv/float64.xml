<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncFloat64Port" structureName = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.Float64DriverFactory</factoryName>
    <portName>syncFloat64Port</portName>
    <driverParameters structureName = "float64Driver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncFloat64DeviceControl" structureName = "portDeviceControl" >
    <portName>syncFloat64Port</portName>
    <deviceName>0</deviceName>
<!--
    <traceMask><value>0xfff</value></traceMask>
    <traceIOMask><value>0xfff</value></traceIOMask>
-->
</record>

<record name = "syncFloat64Counter">
    <value type = "double"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calculator>
        <expression>(value+.5)&lt;=10.0? value+.5 : -10.0</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type>
        <rate>0.5</rate>
    </scan>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncFloat64Out</pvname>
                <process>true</process>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncFloat64Out">
    <value type = "double"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvFloat64Output" structureName = "pdrvSupport">
        <portName>syncFloat64Port</portName>
        <deviceName>0</deviceName>
        <timeout>.2</timeout>
    </output>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncFloat64In</pvname>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncFloat64In">
    <value type = "double"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "pdrvFloat64Input" structureName = "pdrvSupport">
        <portName>syncFloat64Port</portName>
        <deviceName>0</deviceName>
        <timeout>.2</timeout>
        <size>20</size>
    </input>
</record>

