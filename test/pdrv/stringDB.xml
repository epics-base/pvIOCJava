<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncOctetDriver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.OctetDriverFactory</factoryName>
    <portName>syncOctetDriver</portName>
    <driverParameters structureName = "octetDriver" >
        <multiDevice>true</multiDevice>
        <delay>0.0</delay>
    </driverParameters>
</record>

<record name = "syncStringCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>10.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1.0</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <units>counts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
    <scan>
        <scan>periodic</scan>
        <rate>0.5</rate>
    </scan>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>syncStringOut</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "syncStringOut" type = "string">
    <output supportName = "pdrvOctetOutput" >
        <configure structureName = "pdrvLink">
            <portName>syncOctetDriver</portName>
            <addr>0</addr>
            <timeout>.2</timeout>
        </configure>
    </output>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>syncStringIn</pvname>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "syncStringIn" type = "string">
    <input supportName = "pdrvOctetInput" >
        <configure structureName = "pdrvLink">
            <portName>syncOctetDriver</portName>
            <addr>0</addr>
            <timeout>.2</timeout>
            <size>20</size>
        </configure>
    </input>
</record>

<record name = "syncStringInInterrupt" type = "string">
    <input supportName = "pdrvOctetInterruptInput" >
        <configure structureName = "pdrvLink">
            <portName>syncOctetDriver</portName>
            <addr>0</addr>
            <process>true</process>
            <size>20</size>
        </configure>
    </input>
</record>

