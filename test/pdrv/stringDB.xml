<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncStringPort" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.OctetDriverFactory</factoryName>
    <portName>syncStringPort</portName>
    <driverParameters structureName = "octetDriver" >
        <multiDevice>true</multiDevice>
        <delay>0.0</delay>
    </driverParameters>
</record>

<record name = "syncStringDeviceControl" type = "portDeviceControl" >
    <portDevice>syncStringPort[0]</portDevice>
<!--
    <traceMask>0x0fff</traceMask>
    <traceIOMask>0x0fff</traceIOMask>
-->
</record>

<record name = "syncStringCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>10.0</value>
            <name>max</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>1.0</value>
            <name>inc</name>
        </element>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>0.5</rate>
    </scan>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncStringOut</pvname>
                <process>true</process>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncStringOut" type = "string">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvOctetOutput" structureName = "pdrvSupport">
        <portName>syncStringPort</portName>
        <addr>0</addr>
        <timeout>.2</timeout>
    </output>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncStringIn</pvname>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncStringIn" type = "string">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "pdrvOctetInput" structureName = "pdrvSupport">
        <portName>syncStringPort</portName>
        <addr>0</addr>
        <timeout>.2</timeout>
        <size>20</size>
    </input>
</record>

<record name = "syncStringInInterrupt" type = "string">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
