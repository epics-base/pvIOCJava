<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncDigitalDriver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.UInt32DigitalDriverFactory</factoryName>
    <portName>syncUInt32DigitalDriver</portName>
    <driverParameters structureName = "uint32DigitalDriver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncDigitalCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>15</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type> 
        <rate>0.5</rate>
    </scan>
    <processAfterStart>true</processAfterStart>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncDigitalOut</pvname>
                <process>true</process>
            </link>
        </value>
    </supportArray>
    <displayCharacteristics structureName = "doubleDisplayCharacteristics" >
        <units>counts</units>
        <limit>
            <low>0.0</low>
            <high>15.0</high>
        </limit>
    </displayCharacteristics>
</record>

<record name = "syncDigitalOut" type = "int">
    <output supportName = "pdrvUInt32DigitalOutput" structureName = "pdrvSupport">
        <portName>syncUInt32DigitalDriver</portName>
        <addr>0</addr>
        <mask>15</mask>
        <timeout>.2</timeout>
    </output>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn1</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn2</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn3</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn4</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn5</pvname>
            </link>
        </value>
    </supportArray>
</record>

<record name = "syncDigitalIn1" type = "digital">
    <digitalIO structureName = "digitalIO">
        <input supportName = "pdrvUInt32DigitalInput" structureName = "pdrvSupport">
            <portName>syncUInt32DigitalDriver</portName>
            <addr>0</addr>
            <mask>0x1</mask>
            <timeout>.2</timeout>
        </input>
        <states>
             <value structureName = "digitalState">
                 <name>off</name>
                 <value>0</value>
                 <severity>none</severity>
             </value>
             <value structureName = "digitalState">
                 <name>on</name>
                 <value>1</value>
                 <severity>minor</severity>
             </value>
        </states>
        <numberOfBits>1</numberOfBits>
        <valueAlarm structureName = "digitalAlarm">
            <active>true</active>
            <changeStateAlarm>none</changeStateAlarm>
        </valueAlarm>
    </digitalIO>
</record>

