<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncDigitalDriver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.UInt32DigitalDriverFactory</factoryName>
    <portName>syncDigitalDigitalDriver</portName>
    <driverParameters structureName = "uint32DigitalDriver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncDigitalDigitalDriverControl" type = "portDeviceControl" >
<!--
    <portDevice>syncDigitalDigitalDriver[0]</portDevice>
    <traceMask>0x0fff</traceMask>
    <traceIOMask>0x0fff</traceIOMask>
-->
</record>

<record name = "syncDigitalCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>15</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type> 
        <rate>0.5</rate>
        <processAfterStart>true</processAfterStart>
    </scan>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncDigitalOut</pvname>
                <process>true</process>
            </link>
        </value>
    </supportArray>
    <display structureName = "display" >
        <units>counts</units>
        <limit>
            <low>0.0</low>
            <high>15.0</high>
        </limit>
    </display>
</record>

<record name = "syncDigitalOut" type = "int">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvUInt32DigitalOutput" structureName = "pdrvSupport">
        <portName>syncDigitalDigitalDriver</portName>
        <addr>0</addr>
        <mask>15</mask>
        <timeout>.2</timeout>
    </output>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn1</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn2</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn3</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn4</pvname>
            </link>
        </value>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn5</pvname>
            </link>
        </value>
    </supportArray>
</record>

<record name = "syncDigitalIn1" type = "enumerated">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "digital" >
        <alarm structureName = "alarm" />
        <input supportName = "pdrvUInt32DigitalInput" structureName = "pdrvSupport">
            <portName>syncDigitalDigitalDriver</portName>
            <addr>0</addr>
            <mask>0x1</mask>
            <timeout>.2</timeout>
        </input>  
        <states supportName = "digitalInput" >
             <value structureName = "digitalState">
                 <name>off</name>
                 <value>0</value>
                 <severity><choice>none</choice></severity>
             </value>
             <value structureName = "digitalState">
                 <name>on</name>
                 <value>1</value>
                 <severity><choice>minor</choice></severity>
             </value>
         </states>
    </input>
    <valueAlarm structureName = "enumeratedAlarm">
            <active>true</active>
            <changeStateAlarm>
                 <severity><choice>none</choice></severity>
                 <message>Change State</message>
             </changeStateAlarm>
    </valueAlarm>
</record>

