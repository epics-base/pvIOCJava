<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncStringPort" structureName = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.OctetDriverFactory</factoryName>
    <portName>syncStringPort</portName>
    <driverParameters structureName = "octetDriver" >
        <delay>0.0</delay>
    </driverParameters>
</record>

<record name = "syncStringDeviceControl" structureName = "portDeviceControl" >
    <portName>syncStringPort</portName>
    <deviceName>0</deviceName>
<!--
    <traceMask><value>0xfff</value></traceMask>
    <traceIOMask><value>0xfff</value></traceIOMask>
-->
</record>

<record name = "syncStringCounter">
    <value type = "double"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calculator>
        <expression>(value+.5)&lt;=10.0? value+.5 : -10.0</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type>
        <rate>0.5</rate>
    </scan>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncStringOut</pvname>
                <process>true</process>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncStringOut">
    <value type = "string"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvOctetOutput" structureName = "pdrvSupport">
        <portName>syncStringPort</portName>
        <deviceName>0</deviceName>
        <timeout>.2</timeout>
    </output>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncStringIn</pvname>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncStringIn">
    <value type = "string"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "pdrvOctetInput" structureName = "pdrvSupport">
        <portName>syncStringPort</portName>
        <deviceName>0</deviceName>
        <timeout>.2</timeout>
        <size>20</size>
    </input>
</record>

