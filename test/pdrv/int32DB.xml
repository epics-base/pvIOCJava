<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncInt32Driver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.Int32DriverFactory</factoryName>
    <portName>syncInt32Driver</portName>
    <driverParameters structureName = "int32Driver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
        <low>-2048</low>
        <high>2047</high>
    </driverParameters>
</record>

<record name = "syncInt32Counter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>-2048</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>2047</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>100</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <units>counts</units>
    <scan>
        <scan>periodic</scan>
        <rate>0.5</rate>
    </scan>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>syncInt32Out</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "syncInt32Out" type = "double">
    <output supportName = "pdrvInt32Output" >
        <configure structureName = "pdrvLink">
            <portName>syncInt32Driver</portName>
            <addr>0</addr>
            <timeout>.2</timeout>
        </configure>
    </output>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>syncInt32In</pvname>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "syncInt32In" type = "double">
    <input supportName = "pdrvInt32Input" >
        <configure structureName = "pdrvLink">
            <portName>syncInt32Driver</portName>
            <addr>0</addr>
            <timeout>.2</timeout>
            <size>20</size>
        </configure>
    </input>
</record>

<record name = "syncInt32InInterrupt" type = "double">
    <input supportName = "pdrvInt32InterruptInput" >
        <configure structureName = "pdrvLink">
            <portName>syncInt32Driver</portName>
            <addr>0</addr>
            <process>true</process>
            <size>20</size>
        </configure>
    </input>
</record>

<record name = "syncInt32InAverage" type = "double">
    <input supportName = "pdrvInt32AverageInput" >
        <configure structureName = "pdrvLink">
            <portName>syncInt32Driver</portName>
            <addr>0</addr>
            <size>20</size>
        </configure>
    </input>
    <scan>
        <scan>periodic</scan>
        <rate>5.0</rate>
    </scan>
</record>

