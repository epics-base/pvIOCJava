<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncInt32Driver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.Int32DriverFactory</factoryName>
    <portName>syncInt32Driver</portName>
    <driverParameters structureName = "int32Driver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
        <low>-2048</low>
        <high>2047</high>
    </driverParameters>
</record>

<record name = "syncInt32DriverControl" type = "portDeviceControl" >
<!--
    <portDevice>syncInt32Driver[0]</portDevice>
    <traceMask>0x0fff</traceMask>
    <traceIOMask>0x0fff</traceIOMask>
-->
</record>

<record name = "syncInt32Counter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>-2048</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>2047</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>100</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>0.5</rate>
    </scan>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncInt32Out</pvname>
                <process>true</process>
            </link>
        </value>
    </supportArray>
</record>

<record name = "syncInt32Out" type = "double">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvInt32Output" structureName = "pdrvSupport">
        <portName>syncInt32Driver</portName>
        <addr>0</addr>
        <timeout>.2</timeout>
    </output>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncInt32In</pvname>
            </link>
        </value>
    </supportArray>
</record>

<record name = "syncInt32In" type = "double">
    <alarm structureName = "alarm" />
