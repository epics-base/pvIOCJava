            <portName>syncUInt32DigitalDriver</portName>
            <addr>0</addr>
            <mask>0x4</mask>
            <timeout>.2</timeout>
        </configure>
    </input>
    <valueAlarm>
        <configure structureName = "booleanAlarm">
          <active>true</active>
