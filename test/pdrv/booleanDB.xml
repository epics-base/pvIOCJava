<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncBooleanDriver" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.UInt32DigitalDriverFactory</factoryName>
    <portName>syncUInt32DigitalDriver</portName>
    <driverParameters structureName = "uint32DigitalDriver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncBooleanCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>32767</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>1</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <units>counts</units>
    <scan>
        <scan>periodic</scan>
        <rate>0.5</rate>
    </scan>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>syncBooleanOut</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
    </linkArray>
</record>

<record name = "syncBooleanOut" type = "int">
    <output supportName = "pdrvUInt32DigitalOutput" >
        <configure structureName = "pdrvLink">
            <portName>syncUInt32DigitalDriver</portName>
            <addr>0</addr>
            <mask>32767</mask>
            <timeout>.2</timeout>
        </configure>
    </output>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>syncBooleanIn1</pvname>
                </configure>
            </link>
        </value>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>syncBooleanIn2</pvname>
                </configure>
            </link>
        </value>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>syncBooleanIn3</pvname>
                </configure>
            </link>
        </value>

    </linkArray>
</record>

<record name = "syncBooleanIn1" type = "boolean">
    <input supportName = "pdrvUInt32DigitalInput" >
        <configure structureName = "pdrvLink">
            <portName>syncUInt32DigitalDriver</portName>
            <addr>0</addr>
            <mask>0x1</mask>
            <timeout>.2</timeout>
        </configure>
    </input>
    <valueAlarm>
        <configure structureName = "booleanAlarm">
          <active>true</active>
          <falseAlarm>minor</falseAlarm>
          <trueAlarm>major</trueAlarm>
        </configure>
    </valueAlarm>
</record>

<record name = "syncBooleanIn2" type = "boolean">
