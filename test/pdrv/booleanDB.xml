<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncBooleanPort" type = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.UInt32DigitalDriverFactory</factoryName>
    <portName>syncBooleanPort</portName>
    <driverParameters structureName = "uint32DigitalDriver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncBooleanDeviceControl" type = "portDeviceControl" >
    <portDevice>syncBooleanPort[0]</portDevice>
<!--
    <traceMask>0x0fff</traceMask>
    <traceIOMask>0x0fff</traceIOMask>
-->
</record>

<record name = "syncBooleanCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcDoubleArg">
            <value>0</value>
            <name>min</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>32767</value>
            <name>max</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>1</value>
            <name>inc</name>
        </element>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
<!--
-->
        <rate>0.5</rate>
    </scan>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncBooleanOut</pvname>
                <process>true</process>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncBooleanOut" type = "int">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvUInt32DigitalOutput" structureName = "pdrvSupport">
        <portName>syncBooleanPort</portName>
        <addr>0</addr>
        <mask>32767</mask>
        <timeout>.2</timeout>
    </output>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncBooleanIn1</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncBooleanIn2</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncBooleanIn3</pvname>
            </link>
        </element>

    </supportArray>
</record>

