<?xml version="1.0" ?>
<IOCDatabase>

<record name = "syncDigitalPort" structureName = "portCreate">
    <factoryName>org.epics.ioc.pdrv.testDriver.UInt32DigitalDriverFactory</factoryName>
    <portName>syncDigitalPort</portName>
    <driverParameters structureName = "uint32DigitalDriver" >
        <delay>0.0</delay>
        <numberRegisters>2</numberRegisters>
    </driverParameters>
</record>

<record name = "syncDigitalPortControl" structureName = "portDeviceControl" >
    <portName>syncDigitalPort</portName>
    <deviceName>0</deviceName>
<!--
    <traceMask><value>0xfff</value></traceMask>
    <traceIOMask><value>0xfff</value></traceIOMask>
-->
</record>

<record name = "syncDigitalCounter">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calculator>
        <expression>(value+1)&lt;=15 ? value+1 : 0</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type> 
        <rate>0.5</rate>
        <processAfterStart>true</processAfterStart>
    </scan>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>syncDigitalOut</pvname>
                <process>true</process>
            </link>
        </element>
    </supportArray>
    <display structureName = "display" >
        <units>counts</units>
        <limit>
            <low>0.0</low>
            <high>15.0</high>
        </limit>
    </display>
</record>

<record name = "syncDigitalOut">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <output supportName = "pdrvUInt32DigitalOutput" structureName = "pdrvSupport">
        <portName>syncDigitalPort</portName>
        <deviceName>0</deviceName>
        <mask>15</mask>
        <timeout>.2</timeout>
    </output>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn1</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn2</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn3</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn4</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>syncDigitalIn5</pvname>
            </link>
        </element>
    </supportArray>
</record>

<record name = "syncDigitalIn1">
    <value structureName = "enumerated"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "digital" >
        <alarm structureName = "alarm" />
        <input supportName = "pdrvUInt32DigitalInput" structureName = "pdrvSupport">
            <portName>syncDigitalPort</portName>
            <deviceName>0</deviceName>
            <mask>0x1</mask>
            <timeout>.2</timeout>
        </input>  
        <states supportName = "digitalInput" >
             <element structureName = "digitalState">
                 <name>off</name>
                 <value>0</value>
                 <severity><choice>none</choice></severity>
             </element>
             <element structureName = "digitalState">
                 <name>on</name>
                 <value>1</value>
                 <severity><choice>minor</choice></severity>
                 <message>on is minor alarm</message>
             </element>
         </states>
    </input>
    <valueAlarm structureName = "enumeratedAlarm">
            <active>true</active>
            <changeStateAlarm>
                 <severity><choice>none</choice></severity>
                 <message>Change State</message>
             </changeStateAlarm>
    </valueAlarm>
</record>

