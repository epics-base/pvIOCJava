<?xml version="1.0" ?>
<IOCDatabase>
<record name = "floatCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>11.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>0.2</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>floatRecord</pvname>
            </link>
        </value>
    </supportArray>
    <units>counts</units>
    <displayLimit>
        <low>0.0</low>
        <high>11.0</high>
    </displayLimit>
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "floatRecord" type = "float">
    <input supportName = "inputSupport" structureName = "inputSupport">
        <pvname>floatCounter</pvname>
        <process>false</process>
    </input>
    <units>volts</units>
    <valueAlarm  structureName = "floatAlarm">
      <active>true</active>
      <interval>
          <value structureName = "floatAlarmInterval">
              <value>2.0</value>
              <severity><choice>major</choice></severity>
          </value>
          <value structureName = "floatAlarmInterval">
              <value>4.0</value>
              <severity><choice>minor</choice></severity>
          </value>
          <value structureName = "floatAlarmInterval">
              <value>6.0</value>
              <severity><choice>none</choice></severity>
          </value>
          <value structureName = "floatAlarmInterval">
              <value>8.0</value>
              <severity><choice>minor</choice></severity>
          </value>
          <value structureName = "floatAlarmInterval">
              <value>10.0</value>
              <severity><choice>major</choice></severity>
          </value>
      </interval>
      <outOfRange>invalid</outOfRange>
      <hystersis>0.3</hystersis>
    </valueAlarm>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>
