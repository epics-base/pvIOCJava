<?xml version="1.0" ?>
<IOCDatabase>
<record name = "floatCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>11.0</value>
            <name>max</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>0.2</value>
            <name>inc</name>
        </element>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>floatRecord</pvname>
            </link>
        </element>
    </supportArray>
    <display structureName = "display" >
        <units>counts</units>
        <limit>
            <low>0.0</low>
            <high>11.0</high>
        </limit>
    </display>
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "floatRecord" type = "float">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "inputSupport" structureName = "inputSupport">
        <pvname>floatCounter</pvname>
        <process>false</process>
    </input>
    <valueAlarm  structureName = "floatAlarm">
      <active>true</active>
      <interval>
          <element structureName = "floatAlarmInterval">
              <value>2.0</value>
              <severity><choice>major</choice></severity>
              <message>LOWLOW</message>
          </element>
          <element structureName = "floatAlarmInterval">
              <value>4.0</value>
              <severity><choice>minor</choice></severity>
              <message>LOW</message>
          </element>
          <element structureName = "floatAlarmInterval">
              <value>6.0</value>
              <severity><choice>none</choice></severity>
          </element>
          <element structureName = "floatAlarmInterval">
              <value>8.0</value>
              <severity><choice>minor</choice></severity>
              <message>HIGH</message>
          </element>
          <element structureName = "floatAlarmInterval">
              <value>10.0</value>
              <severity><choice>major</choice></severity>
              <message>HIHI</message>
          </element>
      </interval>
      <outOfRange><choice>invalid</choice></outOfRange>
      <hystersis>0.3</hystersis>
    </valueAlarm>
    <display structureName = "display" >
        <units>counts</units>
        <limit>
            <low>0.0</low>
            <high>10.0</high>
        </limit>
    </display>
</record>
