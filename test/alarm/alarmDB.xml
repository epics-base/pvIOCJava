<?xml version="1.0" ?>
<database>
<record name = "floatCounter">
    <scalar name = "value"  type = "float" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
       <structure name = "calcArgs">
          <structure name = "min">
            <scalar name = "value" type = "double" >0.0</scalar>
          </structure>
          <structure name = "max">
            <scalar name = "value" type = "double" >11.0</scalar>
          </structure>
          <structure name = "inc">
            <scalar name = "value" type = "double" >0.2</scalar>
          </structure>
       </structure>
       <structure name = "calculator">
         <scalar name = "expression">(value+inc)&lt;=max ? value+inc : min</scalar>
       </structure>
    </structure>
    <structure name = "process" type = "generic">
        <structure name = "link" type = "processSupport">
            <scalar name = "pvname">floatRecord</scalar>
        </structure>
    </structure>
    <structure name = "display" type = "display" >
        <scalar name = "units">increment</scalar>
        <structure name = "limit">
            <scalar name = "low">0.0</scalar>
            <scalar name = "high">11.0</scalar>
        </structure>
    </structure>
    <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">periodic</scalar></structure>
        <scalar name = "rate">1.0</scalar>
    </structure>
</record>

