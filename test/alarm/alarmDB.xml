            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>intRecord</pvname>
                </configure>
            </link>
        </value>
    </linkArray>
    <units>counts</units>
    <displayLimit>
        <low>0.0</low>
        <high>11.0</high>
    </displayLimit>
    <scan>
        <scan>periodic</scan>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "intRecord" type = "int">
    <input supportName = "inputLink" >
        <configure structureName = "inputLink">
            <pvname>intCounter</pvname>
            <process>false</process>
        </configure>
    </input>
    <units>volts</units>
    <intAlarm>
        <configure structureName = "intAlarm">
          <active>true</active>
          <interval>
              <value structureName = "intAlarmInterval">
                  <value>2</value>
                  <severity>major</severity>
              </value>
              <value structureName = "intAlarmInterval">
                  <value>4</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "intAlarmInterval">
                  <value>6</value>
                  <severity>none</severity>
              </value>
              <value structureName = "intAlarmInterval">
                  <value>8</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "intAlarmInterval">
                  <value>10</value>
                  <severity>major</severity>
