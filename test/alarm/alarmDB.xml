<?xml version="1.0" ?>
<IOCDatabase>
<record name = "floatCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>11.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>0.2</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>true</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>floatRecord</pvname>
                </configure>
            </link>
        </value>
    </linkArray>
    <units>counts</units>
    <displayLimit>
        <low>0.0</low>
        <high>11.0</high>
    </displayLimit>
    <scan>
        <scan>periodic</scan>
        <rate>1.0</rate>
    </scan>
</record>

<record name = "floatRecord" type = "float">
    <input supportName = "inputLink" >
        <configure structureName = "inputLink">
            <pvname>floatCounter</pvname>
            <process>false</process>
        </configure>
    </input>
    <units>volts</units>
    <valueAlarm>
        <configure structureName = "floatAlarm">
          <active>true</active>
          <interval>
              <value structureName = "floatAlarmInterval">
                  <value>2.0</value>
                  <severity>major</severity>
              </value>
              <value structureName = "floatAlarmInterval">
                  <value>4.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "floatAlarmInterval">
                  <value>6.0</value>
                  <severity>none</severity>
              </value>
              <value structureName = "floatAlarmInterval">
                  <value>8.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "floatAlarmInterval">
                  <value>10.0</value>
                  <severity>major</severity>
              </value>
          </interval>
          <outOfRange>invalid</outOfRange>
          <hystersis>0.3</hystersis>
        </configure>
    </valueAlarm>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>
