<?xml version="1.0" ?>
<IOCDatabase>

<record name = "enumeratedCounter">
    <value type = "byte"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calculator structureName = "expressionCalculator" >
        <expression>(value+1&lt;=3)?(value+1):0</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type> 
        <rate>1.0</rate>
        <processAfterStart>true</processAfterStart>
    </scan>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                <pvname>enumerated.value.index</pvname>
                <process>true</process>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                <pvname>enumeratedInput</pvname>
            </link>
        </element>
    </supportArray>
</record>


<record name = "enumerated">
    <value structureName = "enumerated"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "digital">
        <alarm structureName = "alarm" />
        <states supportName = "digitalOutput" >
             <element structureName = "digitalState">
                 <name>zero</name>
                 <value>0</value>
                 <severity><choice>major</choice></severity>
                 <message>ZERO ALARM</message>
             </element>
             <element structureName = "digitalState">
                 <name>one</name>
                 <value>1</value>
                 <severity><choice>none</choice></severity>
             </element>
             <element structureName = "digitalState">
                 <name>two</name>
                 <value>2</value>
                 <severity><choice>none</choice></severity>
             </element>
             <element structureName = "digitalState">
                 <name>three</name>
                <value>3</value>
                <severity><choice>minor</choice></severity>
                 <message>THREE ALARM</message>
             </element>
             <element structureName = "digitalState">
                 <name>four</name>
                 <value>4</value>
                 <severity><choice>major</choice></severity>
                 <message>FOUR ALARM</message>
             </element>
        </states>
    </input>
    <valueAlarm structureName = "enumeratedAlarm">
            <active>true</active>
            <changeStateAlarm>
                 <severity><choice>none</choice></severity>
                 <message>Change State</message>
             </changeStateAlarm>
    </valueAlarm>
</record>


<record name = "enumeratedInput" >
    <value structureName = "enumerated"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "inputSupport" structureName = "inputSupport">
        <pvname>enumerated.value</pvname>
        <process>false</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>


<record name = "enumeratedMonitor" >
    <value structureName = "enumerated"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumerated.value</pvname>
        <process>true</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>

<record name = "enumeratedInputIndex">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumerated.value.index</pvname>
        <process>true</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>

<record name = "enumeratedMonitorIndex">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumerated.value.index</pvname>
        <process>true</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>

<record name = "enumeratedInputChoice">
    <value type = "string"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumerated.value.choice</pvname>
        <process>true</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>

<record name = "enumeratedMonitorChoice">
    <value type = "string"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumerated.value.choice</pvname>
        <process>true</process>
        <propertyNames>alarm</propertyNames>
    </input>
</record>


