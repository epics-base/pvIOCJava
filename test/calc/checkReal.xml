<?xml version="1.0" ?>
<IOCDatabase>
<record name = "${checkRecord}Check" >
    <value type = "boolean" />
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calcArgArray>
        <element structureName = "calcArg">
            <value type = "${checkType}" />
            <name>a</name>
        </element>
        <element structureName = "calcArg">
            <name>b</name>
            <value type = "${checkType}">${checkValue}</value>
        </element>
      </calcArgArray>
      <calculator >
        <expression>
            (Math.abs(Math.max(a,b))>1e-5) ?
            ((Math.abs(a-b)/Math.abs(Math.max(a,b)))&lt;1e-5 ? true : false)
            : true
        </expression>
      </calculator>
    </input>
    <valueAlarm structureName = "booleanAlarm" >
       <active>true</active>
       <falseMessage>checkExpression Failure</falseMessage>
       <falseAlarm><choice>major</choice></falseAlarm>
    </valueAlarm>
</record>
</IOCDatabase>
