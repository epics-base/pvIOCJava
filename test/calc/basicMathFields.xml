<?xml version="1.0" ?>
<database>
    <scalar name = "value" type = "${checkType}" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
       <structure name = "calcArgs">
          <structure name = "a">
            <scalar name = "value" type = "${checkType}" >4</scalar>
          </structure>
          <structure name = "b">
            <scalar name = "value" type = "${checkType}" >2</scalar>
          </structure>
          <structure name = "c">
            <scalar name = "value" type = "${checkType}" >6</scalar>
          </structure>
          <structure name = "d">
            <scalar name = "value" type = "${checkType}" >4</scalar>
          </structure>
          <structure name = "e">
            <scalar name = "value" type = "${checkType}" >3</scalar>
          </structure>
       </structure>
       <structure name = "calculator">
         <scalar name = "expression">a+b*c/d+e</scalar>
       </structure>
    </structure>
    <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">periodic</scalar></structure>
        <scalar name = "processAfterStart">true</scalar>
    </structure>
</database>
