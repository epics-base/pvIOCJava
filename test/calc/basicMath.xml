<?xml version="1.0" ?>
<IOCDatabase>

<substitute fromTo = "checkRecord=basicMathByte,checkType=byte,checkValue=10" />
<record name = "${checkRecord}">
    <value type = "${checkType}" />
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calcArgArray>
        <element structureName = "calcArg">
            <value type = "${checkType}">4</value>
            <name>a</name>
        </element>
        <element structureName = "calcArg">
            <value type = "${checkType}">2</value>
            <name>b</name>
        </element>
        <element structureName = "calcArg">
            <value type = "${checkType}">6</value>
            <name>c</name>
        </element>
        <element structureName = "calcArg">
            <value type = "${checkType}">4</value>
            <name>d</name>
        </element>
        <element structureName = "calcArg">
            <value type = "${checkType}">3</value>
            <name>e</name>
        </element>
      </calcArgArray>
      <calculator >
        <expression>
        a+b*c/d+e
        </expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <processAfterStart>true</processAfterStart>
    </scan>
<include href = "checkLink.xml" />
</record>
<include href = "checkInteger.xml" />

