<?xml version="1.0" ?>
<IOCDatabase>

<substitute fromTo = "checkRecord=basicMathByte,checkType=Byte,checkValue=10" />
<record name = "${checkRecord}" type = "calc${checkType}">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcByteArg">
            <value>4</value>
            <name>a</name>
        </element>
        <element structureName = "calcByteArg">
            <value>2</value>
            <name>b</name>
        </element>
        <element structureName = "calcByteArg">
            <value>6</value>
            <name>c</name>
        </element>
        <element structureName = "calcByteArg">
            <value>4</value>
            <name>d</name>
        </element>
        <element structureName = "calcByteArg">
            <value>3</value>
            <name>e</name>
        </element>
    </calcArgArray>
    <calculator structureName = "expressionCalculator" >
        <expression>
        a+b*c/d+e
        </expression>
    </calculator>
    <scan>
        <processAfterStart>true</processAfterStart>
    </scan>
<include href = "checkLink.xml" />
</record>
<include href = "checkInteger.xml" />

