<?xml version="1.0" ?>
<database>

<substitute fromTo = "checkRecord=string,checkType=string" />
<substitute from = "checkValue" to = "This is 2 strings" />
<record name = "${checkRecord}" >
    <scalar name = "value" type = "${checkType}" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
       <structure name = "calcArgs">
          <structure name = "a">
            <scalar name = "value" type = "string" >This is</scalar>
          </structure>
          <structure name = "b">
            <scalar name = "value" type = "int" >2</scalar>
          </structure>
          
       </structure>
       <structure name = "calculator">
         <scalar name = "expression">
             a+" " + b+" strings"
         </scalar>
       </structure>
    </structure>
    <structure name = "scan" type = "scan">
        <scalar name = "processAfterStart">true</scalar>
    </structure>
    <include href = "checkLink.xml" />
</record>
<include href = "checkString.xml" />


<!--
-->

</database>
