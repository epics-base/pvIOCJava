<?xml version="1.0" ?>
<IOCDatabase>
<record name = "${checkRecord}Check" >
    <value  type = "boolean" />
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calcArgArray>
        <element structureName = "calcArg">
            <value type = "string" />
            <name>a</name>
        </element>
        <element structureName = "calcArg">
            <name>b</name>
            <value type = "string" >${checkValue}</value>
        </element>
      </calcArgArray>
      <calculator structureName = "expressionCalculator" >
        <expression>
            (a==b) ? true : false
        </expression>
      </calculator>
    </input>
    <valueAlarm structureName = "booleanAlarm" >
       <active>true</active>
       <falseMessage>checkExpression Failure</falseMessage>
       <falseAlarm><choice>major</choice></falseAlarm>
    </valueAlarm>
</record>
</IOCDatabase>
