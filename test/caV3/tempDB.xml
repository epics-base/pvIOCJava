<?xml version="1.0" ?>
<IOCDatabase>

<record name = "byteCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>11.0</value>
            <name>max</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>1.0</value>
            <name>inc</name>
        </element>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>


<record name = "byteOut" type = "byte">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>byteCounter</pvname>
        <process>true</process>
    </input>
    <output supportName = "outputSupport" structureName = "outputSupport">
        <providerName>caV3</providerName>
        <pvname>byte</pvname>
    </output>
