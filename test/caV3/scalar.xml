    <scalar name = "value" type = "int"/>
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "monitorSupport">
        <scalar name = "pvname">enumCounter${num}</scalar>
        <scalar name = "process">true</scalar>
    </structure>
    <structure name = "output" type = "outputSupport">
        <scalar name = "providerName">caV3</scalar>
        <scalar name = "pvname">enum${num}.value.index</scalar>
    </structure>
    <structure name = "process0" type = "processSupport">
       <scalar name = "wait" type = "boolean">false</scalar>
       <scalar name = "pvname">enumIn${num}</scalar>
    </structure>
    <structure name = "process1" type = "processSupport">
       <scalar name = "wait" type = "boolean">false</scalar>
       <scalar name = "pvname">enumInIndex${num}</scalar>
    </structure>
    <structure name = "process2" type = "processSupport">
       <scalar name = "wait" type = "boolean">false</scalar>
       <scalar name = "pvname">enumInChoice${num}</scalar>
    </structure>
</record>

