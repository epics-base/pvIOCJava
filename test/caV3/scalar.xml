<?xml version="1.0" ?>
<IOCDatabase>

<record name = "enumCounter${num}">
    <value type = "byte" />
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calcArgArray>
        <element structureName = "calcArg">
            <value type = "byte">0</value>
            <name>min</name>
        </element>
        <element structureName = "calcArg">
            <value type = "byte">3</value>
            <name>max</name>
        </element>
        <element structureName = "calcArg">
            <value type = "byte">1</value>
            <name>inc</name>
        </element>
      </calcArgArray>
      <calculator structureName = "expressionCalculator" >
        <expression>((value+inc)&lt;=max)?(value+inc):min</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>
<record name = "enumOut${num}">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumCounter${num}</pvname>
        <process>true</process>
    </input>
    <output supportName = "outputSupport" structureName = "outputSupport">
        <providerName>caV3</providerName>
        <pvname>enum${num}.value.index</pvname>
    </output>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumIn${num}</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumInIndex${num}</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumInChoice${num}</pvname>
            </link>
        </element>
    </supportArray>
</record>

<record name = "enumIn${num}">
    <value structureName = "enumerated"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "inputSupport" structureName = "inputSupport">
        <providerName>caV3</providerName>
