<?xml version="1.0" ?>
<IOCDatabase>
<record name = "arrayIncrementer" type = "calcDoubleArray">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <value capacity = "5">
        <element>0.0 1.0 2.0 3.0 4.0</element>
    </value>
    <calculator supportName = "arrayIncrementCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <display structureName = "display" >
        <description>Sample Description</description>
        <format>%f</format>
        <resolution>1</resolution>
        <units>volts</units>
    </display>
</record>

<record name = "byteArrayOut" type = "byteArray" >
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>arrayIncrementer</pvname>
        <process>true</process>
    </input>
    <output supportName = "outputSupport" structureName = "outputSupport">
        <providerName>caV3</providerName>
        <pvname>byteArray</pvname>
    </output>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>byteArrayIn</pvname>
