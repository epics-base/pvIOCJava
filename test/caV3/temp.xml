<?xml version="1.0" ?>
<database>
<record name = "enumCounter01">
    <scalar name = "value" type = "byte" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
      <structure name = "calculator">
        <scalar name ="expression">(value+1)&lt;=3 ? value+1 : 0</scalar>
      </structure>
    </structure>
    <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">passive</scalar></structure>
        <scalar name = "rate">1.0</scalar>
    </structure>
</record>

<record name = "enumOut01">
    <scalar name = "value" type = "int"/>
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "monitorSupport">
        <scalar name = "pvname">enumCounter01</scalar>
        <scalar name = "process">true</scalar>
    </structure>
    <structure name = "output" type = "outputSupport">
        <scalar name = "providerName">caV3</scalar>
        <scalar name = "pvname">enum01.value.index</scalar>
    </structure>
    <structure name = "process0" type = "processSupport">
       <scalar name = "wait" type = "boolean">false</scalar>
       <scalar name = "pvname">enumIn01</scalar>
    </structure>
</record>

<record name = "enumIn01">
    <structure name = "value" type = "enumerated"/>
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "inputSupport">
        <scalar name = "providerName">caV3</scalar>
        <scalar name = "pvname">enum01.value</scalar>
        <scalar name = "propertyNames">alarm</scalar>
    </structure>
</record>

