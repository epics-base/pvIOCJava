<IOCDatabase>

<record name = "enumCounter" type = "calcDouble">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <calcArgArray>
        <element structureName = "calcDoubleArg">
            <value>0</value>
            <name>min</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>3</value>
            <name>max</name>
        </element>
        <element structureName = "calcDoubleArg">
            <value>1</value>
            <name>inc</name>
        </element>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
        <processAfterStart>true</processAfterStart>
    </scan>
</record>
<record name = "enumOut" type = "int">
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>enumCounter</pvname>
        <process>true</process>
    </input>
    <output supportName = "outputSupport" structureName = "outputSupport">
        <providerName>caV3</providerName>
        <pvname>enum.value.index</pvname>
    </output>
    <supportArray>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumIn</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumInIndex</pvname>
            </link>
        </element>
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>enumInChoice</pvname>
            </link>
        </element>
    </supportArray>

</record>

