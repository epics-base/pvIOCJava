<?xml version="1.0" ?>
<IOCDatabase>
<record name = "arrayIncrementer${num}">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <value elementType = "double" capacity = "5">
        <element>0.0 1.0 2.0 3.0 4.0</element>
    </value>
    <input structureName = "calculation" >
        <calculator supportName = "arrayIncrementCalculator" />
    </input>
    <scan structureName = "scan" >
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <display structureName = "display" >
        <description>Sample Description</description>
        <format>%f</format>
        <resolution>1</resolution>
        <units>volts</units>
    </display>
</record>

<record name = "byteArrayOut${num}" >
    <value elementType = "byte"/>
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <input supportName = "monitorSupport" structureName = "monitorSupport">
        <pvname>arrayIncrementer${num}</pvname>
        <process>true</process>
    </input>
    <output supportName = "outputSupport" structureName = "outputSupport">
        <providerName>caV3</providerName>
        <pvname>byteArray${num}</pvname>
    </output>
    <supportArray elementType = "structure" supportName = "supportArray" >
        <element structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport"  structureName = "processSupport">
                <pvname>byteArrayIn${num}</pvname>
            </link>
        </element>
    </supportArray>

</record>

<record name = "byteArrayIn${num}">
