<?xml version="1.0" ?>
<IOCDatabase>


<record name = "aoCounter" type = "calcDouble">
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>10.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>0.1</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <units>counts</units>
    <scan>
        <scan>periodic</scan>
        <rate>1.0</rate>
    </scan>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>ao</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
    </linkArray>

</record>

<record name = "ao" type = "aoDouble" >
    <output>
        <linearConvert>
            <deviceHigh>2047</deviceHigh>
            <deviceLow>-2048</deviceLow>
            <engUnitsLow>0.0</engUnitsLow>
            <engUnitsHigh>10.0</engUnitsHigh>
        </linearConvert>
        <output supportName = "outputLink" >
            <configure structureName = "outputLink">
                <pvname>aoRaw</pvname>
                <process>true</process>
            </configure>
        </output>

    </output>
    <valueAlarm>
        <configure structureName = "doubleAlarm">
          <active>true</active>
          <interval>
              <value structureName = "doubleAlarmInterval">
                  <value>2.0</value>
                  <severity>major</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>4.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>6.0</value>
                  <severity>none</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>8.0</value>
                  <severity>minor</severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>10.0</value>
                  <severity>major</severity>
              </value>
          </interval>
          <outOfRange>invalid</outOfRange>
          <hystersis>0.1</hystersis>
        </configure>
    </valueAlarm>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>

<record name = "aoRaw" type = "int">
</record>


</IOCDatabase>
