<?xml version="1.0" ?>
<IOCDatabase>
<record name = "counter" type = "counterRecord">
    <min>0.0</min>
    <max>1e9</max>
    <inc>1.0</inc>
    <linkArray>
        <value structureName = "linkArrayElement">
            <wait>true</wait>
            <link  supportName = "processLink" >
                <configure structureName = "processLink">
                    <pvname>double01</pvname>
                </configure>
            </link>
        </value>
        <value structureName = "linkArrayElement">
            <wait>false</wait>
            <link  supportName = "outputLink" >
                <configure structureName = "outputLink">
                    <pvname>double02</pvname>
                    <process>true</process>
                </configure>
            </link>
        </value>
    </linkArray>
</record>
<record name = "double01" type = "doubleRecord">
    <input supportName = "inputLink" >
        <configure structureName = "inputLink">
            <pvname>counter</pvname>
            <process>false</process>
        </configure>
    </input>
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>
<record name = "double02" type = "doubleRecord">
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>
<record name = "double03" type = "doubleRecord">
    <input supportName = "monitorLink" >
        <configure structureName = "monitorLink">
            <pvname>counter</pvname>
            <process>true</process>
            <queueCapacity>1</queueCapacity>
        </configure>
    </input>
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
</record>


</IOCDatabase>
