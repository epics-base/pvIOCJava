<?xml version="1.0" ?>
<IOCDatabase>
<record name = "root" type = "counterRecord">
    <min>0.0</min>
    <max>1e9</max>
    <inc>1.0</inc>
    <priority>middle</priority>
    <process supportName = "linkArray" >
        <value  supportName = "processLink" >
            <configure structureName = "processLink">
                <pvname>other01</pvname>
            </configure>
        </value>
        <value supportName = "outputLink" >
            <configure structureName = "outputLink">
                <pvname>other02</pvname>
                <process>true</process>
            </configure>
        </value>
    </process>
</record>
<record name = "other01" type = "doubleRecord">
    <input supportName = "inputLink" >
        <configure structureName = "inputLink">
            <pvname>root</pvname>
            <process>false</process>
        </configure>
    </input>
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
    <priority>middle</priority>
</record>
<record name = "other02" type = "doubleRecord">
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
    <priority>middle</priority>
    <process supportName = "linkArray" >
        <value  supportName = "processLink" >
            <configure structureName = "processLink">
                <pvname>other03</pvname>
            </configure>
        </value>
    </process>
</record>
<record name = "other03" type = "doubleRecord">
    <input supportName = "inputLink" >
        <configure structureName = "inputLink">
            <pvname>root</pvname>
            <process>true</process>
        </configure>
    </input>
    <units>volts</units>
    <displayLimit>
        <low>0.0</low>
        <high>10.0</high>
    </displayLimit>
    <priority>middle</priority>
</record>


</IOCDatabase>
