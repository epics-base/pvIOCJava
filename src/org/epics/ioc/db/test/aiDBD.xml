<?xml version="1.0" ?>
<DBDefinition>

<structure name = "aiRaw" supportName = "noop" >
    <field name = "value" type = "int" >
        <property name = "status" associatedField = "status" />
        <property name = "severity" associatedField = "severity" />
        <property name = "timeStamp" associatedField = "/timeStamp" />
    </field>
    <field name = "status" type = "string" />
    <field name = "severity" type = "menu" menuName = "alarmSeverity" />
    <field name = "input" type = "link" />
</structure>

<structure name = "aiLinear" supportName = "noop" >
    <property name = "rawValue" associatedField = "aiRaw.value" />
    <field name = "value" type = "double" >
        <property name = "status" associatedField = "status" />
        <property name = "severity" associatedField = "severity" />
        <property name = "units" associatedField = "units" />
        <property name = "doubleLimit" associatedField = "doubleLimit" />
        <property name = "timeStamp" associatedField = "/timeStamp" />
    </field>
    <field name = "status" type = "string" />
    <field name = "severity" type = "menu" menuName = "alarmSeverity" />
    <field name = "units" type = "string" />
    <field name = "doubleLimit"
         type = "structure" structureName = "doubleLimit" />
    <field name = "aiRaw" type = "structure" structureName = "aiRaw"/>
    <field name = "linearConvert"
        type = "structure"
        structureName = "linearConvert" 
        supportName = "linearConvertInput" />
</structure>

<recordType name = "aiRecord" supportName = "noop">
    <property name = "value" associatedField = "input.value" />
    <property name = "rawValue" associatedField = "input.rawValue" />
    <property name = "status" associatedField = "input.status" />
    <property name = "severity" associatedField = "input.severity" />
    <field name = "input" type = "structure" structureName = "aiLinear" >
        <property name = "timeStamp" associatedField = "/timeStamp" />
    </field>
    <field name = "priority" type = "menu" menuName = "priority" />
    <field name = "timeStamp" type = "structure" structureName = "timeStamp"/>
    <field name = "scan" type = "structure" structureName = "scan"/>
</recordType>

<recordType name = "inputRecord" supportName = "noop">
    <property name = "value" associatedField = "input.value" />
    <property name = "rawValue" associatedField = "input.rawValue" />
    <property name = "status" associatedField = "input.status" />
    <property name = "severity" associatedField = "input.severity" />
    <field name = "input" type = "structure" structureName = "null"  >
        <property name = "timeStamp" associatedField = "timeStamp" />
    </field>
    <field name = "priority" type = "menu" menuName = "priority" />
    <field name = "timeStamp" type = "structure" structureName = "timeStamp"/>
    <field name = "scan" type = "structure" structureName = "scan"/>
</recordType>


</DBDefinition>
