<?xml version="1.0" ?>
<database>
<record name = "valueOnly" type = "generic">
    <scalar name = "value" type = "int" />
</record>

<record name = "simpleCounter" type = "generic">
    <scalar name = "value" type = "int" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
      <structure name = "calculator">
        <scalar name ="expression">(value+1)&lt;=10 ? value+1 : 0</scalar>
      </structure>
    </structure>
</record>

<record name = "counterWithAlarm" type = "generic">
    <scalar name = "value" type = "int" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "input" type = "calculation" >
      <structure name = "calculator">
        <scalar name ="expression">(value+1)&lt;=10 ? value+1 : 0</scalar>
      </structure>
    </structure>
    <structure name = "valueAlarm" type = "intAlarm">
          <scalar name = "active">true</scalar>
          <structure name = "interval">
              <structure name = "0" type = "intAlarmInterval">
                  <scalar name = "value">2</scalar>
                  <structure name = "severity"><scalar name = "choice">major</scalar></structure>
                  <scalar name = "message">LOWLOW</scalar>
              </structure>
              <structure name = "1" type = "intAlarmInterval">
                  <scalar name = "value">4</scalar>
                  <structure name = "severity"><scalar name = "choice">major</scalar></structure>
                  <scalar name = "message">LOW</scalar>
              </structure>
              <structure name = "2" type = "intAlarmInterval">
                  <scalar name = "value">6</scalar>
                  <structure name = "severity"><scalar name = "choice">major</scalar></structure>
                  <scalar name = "message">none</scalar>
              </structure>
              <structure name = "3" type = "intAlarmInterval">
                  <scalar name = "value">8</scalar>
                  <structure name = "severity"><scalar name = "choice">major</scalar></structure>
                  <scalar name = "message">HIGH</scalar>
              </structure>
              <structure name = "4" type = "intAlarmInterval">
                  <scalar name = "value">10</scalar>
                  <structure name = "severity"><scalar name = "choice">major</scalar></structure>
                  <scalar name = "message">HIGHHIGH</scalar>
              </structure>
          </structure>
          <structure name ="outOfRange"><scalar name = "choice">invalid</scalar></structure>
    </structure> 
</record>


