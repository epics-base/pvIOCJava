<?xml version="1.0" ?>
<IOCDatabase>


<record name = "aiRawCounter" type = "calcDouble">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>-2048.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>2047.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>100.0</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>aiRaw</pvname>
                    <process>true</process>
            </link>
        </value>
    </supportArray>

</record>

<record name = "aiRaw" type = "int">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>true</wait>
            <link  supportName = "processSupport" structureName = "processSupport">
                    <pvname>ai</pvname>
            </link>
        </value>
   </supportArray>
</record>

<record name = "ai" type = "double" >
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <input structureName = "linearConvertInput">
        <linearConvert>
            <deviceHigh>2047</deviceHigh>
            <deviceLow>-2048</deviceLow>
            <engUnitsLow>0.0</engUnitsLow>
            <engUnitsHigh>10.0</engUnitsHigh>
        </linearConvert>
        <input supportName = "inputSupport" structureName = "inputSupport">
                <pvname>aiRaw</pvname>
                <process>false</process>
        </input>

    </input>
    <valueAlarm structureName = "doubleAlarm">
          <active>true</active>
          <interval>
              <value structureName = "doubleAlarmInterval">
                  <value>2.0</value>
                  <severity><choice>major</choice></severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>4.0</value>
                  <severity><choice>minor</choice></severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>6.0</value>
                  <severity><choice>none</choice></severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>8.0</value>
                  <severity><choice>minor</choice></severity>
              </value>
              <value structureName = "doubleAlarmInterval">
                  <value>10.0</value>
                  <severity><choice>major</choice></severity>
              </value>
          </interval>
          <outOfRange><choice>invalid</choice></outOfRange>
          <hystersis>0.1</hystersis>
    </valueAlarm>
    <display structureName = "display" >
        <units>volts</units>
        <limit>
            <low>0.0</low>
            <high>10.0</high>
        </limit>
    </display>
</record>


</IOCDatabase>
