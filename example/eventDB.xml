<?xml version="1.0" ?>
<IOCDatabase>
<record name = "eventCounter">
    <value type = "int"/>
    <alarm structureName = "alarm" />
    <timeStamp structureName = "timeStamp" />
    <input structureName = "calculation" >
      <calculator>
        <expression>value+1</expression>
      </calculator>
    </input>
    <scan structureName = "scan" >
        <type><choice>event</choice></type>
        <eventName>eventTest</eventName>
    </scan>
</record>

<record name = "event">
    <value type = "string" supportName = "event">eventTest</value>
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <scan structureName = "scan">
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
</record>

</IOCDatabase>
