<?xml version="1.0" ?>
<IOCDatabase>

<record name = "aoIncremental" type = "aoIncrementalDouble" >
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <desired>
        <value createName = "controlLimit" />
        <rateOfChange>.02</rateOfChange>
        <incremental>true</incremental>
        <control structureName = "control" >
            <limit>
                <low>0.0</low>
                <high>8.0</high>
            </limit>
        </control>
    </desired>
    <output structureName = "linearConvertOutput" >
        <linearConvert>
            <engUnitsLow>0.0</engUnitsLow>
            <engUnitsHigh>10.0</engUnitsHigh>
            <deviceLow>0</deviceLow>
            <deviceHigh>4096</deviceHigh>
        </linearConvert>
    </output>
    <display structureName = "display" >
        <units>volts</units>
        <limit>
            <low>0.0</low>
            <high>10.0</high>
        </limit>
    </display>
</record>


</IOCDatabase>
