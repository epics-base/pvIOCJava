<?xml version="1.0" ?>
<IOCDatabase>


<record name = "aoCounter" type = "calcDouble">
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <calcArgArray>
        <value structureName = "calcDoubleArg">
            <value>0.0</value>
            <name>min</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>10.0</value>
            <name>max</name>
        </value>
        <value structureName = "calcDoubleArg">
            <value>0.1</value>
            <name>inc</name>
        </value>
    </calcArgArray>
    <calculator supportName = "counterCalculator" />
    <scan>
        <type><choice>periodic</choice></type>
        <rate>1.0</rate>
    </scan>
    <supportArray>
        <value structureName = "supportArrayElement">
            <wait>false</wait>
            <link  supportName = "outputSupport" structureName = "outputSupport">
                    <pvname>ao</pvname>
                    <process>true</process>
            </link>
        </value>
    </supportArray>

</record>

<record name = "ao" type = "double" >
    <timeStamp structureName = "timeStamp" />
    <alarm structureName = "alarm" />
    <output structureName = "linearConvertOutput">
        <linearConvert>
            <deviceHigh>2047</deviceHigh>
            <deviceLow>-2048</deviceLow>
            <engUnitsLow>0.0</engUnitsLow>
            <engUnitsHigh>10.0</engUnitsHigh>
        </linearConvert>
        <output supportName = "outputSupport" structureName = "outputSupport">
                <pvname>aoRaw</pvname>
                <process>true</process>
        </output>

    </output>
</record>

<record name = "aoRaw" type = "int">
</record>


</IOCDatabase>
