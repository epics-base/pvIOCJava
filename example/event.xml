<?xml version="1.0" ?>
<database>
<record name = "eventCounter">
    <scalar name = "value" type = "int"/>
    <structure name = "alarm" type = "alarm" />
    <structure name = "timeStamp" type = "timeStamp" />
     <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">event</scalar></structure>
        <scalar name = "eventName">eventTest</scalar>
    </structure>
    <structure name = "input" type = "calculation" >
      <structure name = "calculator">
        <scalar name ="expression">value+1</scalar>
      </structure>
    </structure>
</record>

<record name = "event">
    <scalar name = "value" type = "string" >
        <auxInfo name = "supportFactory" type = "string">eventFactory</auxInfo>
        eventTest
    </scalar>
    <structure name = "timeStamp" type = "timeStamp" />
    <structure name = "alarm" type = "alarm" />
    <structure name = "scan" type = "scan">
        <structure name = "type"><scalar name = "choice">periodic</scalar></structure>
        <scalar name = "rate">1.0</scalar>
    </structure>
</record>

</database>
